"use strict";function partilhasNoticia(){if($("#partilhaSticky").length!==0){var t=$("#partilhasAnchor"),n=t.offset();$(window).scroll(function(){$(this).scrollTop()>n.top-100?$("#partilhaSticky").addClass("utilitiesFixed"):$(this).scrollTop()<=n.top&&$("#partilhaSticky").removeClass("utilitiesFixed")})}}function partilhas_FixarBotom(){$("#partilhaSticky").offset().top+$("#partilhaSticky").height()>=$(".lineFinal").offset().top&&($("#partilhaSticky").removeClass("utilitiesFixed"),$("#partilhaSticky").addClass("utilitiesFixedBottom"));$(document).scrollTop()+window.innerHeight<$(".lineFinal").offset().top+70&&($("#partilhaSticky").removeClass("utilitiesFixedBottom"),$("#partilhaSticky").addClass("utilitiesFixed"))}var jRes=jRespond([{label:"mobile",enter:0,exit:767},{label:"tablet",enter:768,exit:991},{label:"pc",enter:992,exit:1e4}]);jRes.addFunc([{breakpoint:"mobile",enter:function(){$(".commentsBtn").click(function(){$("html, body").animate({scrollTop:$(".mostraComentarios").offset().top-($("header").height()+60)},"slow");$(".mostraComentarios span").hasClass("glyphicon glyphicon-chevron-down")&&($("#comentarios").addClass("collapse in"),$("#comentarios").css("height","inherit"),$(".mostraComentarios span").removeClass("glyphicon glyphicon-chevron-down"),$(".mostraComentarios span").addClass("glyphicon glyphicon-chevron-up"))});setTimeout(function(){Galleria.loadTheme("/scripts/themes/classic/galleria.classic.min.js");Galleria.configure({height:.9,imageCrop:!1,responsive:!0,showCounter:!1,thumbnails:!1,suggestedUrl:"#",thumbFit:!1,thumbMargin:0,debug:!0,extend:function(){Galleria.get(0).$("info-link").click()}})},500);$("#comentarios").addClass("collapse");$(".mostraComentarios").click(function(){$(".mostraComentarios span").hasClass("glyphicon glyphicon-chevron-down")?($(".mostraComentarios span").removeClass("glyphicon glyphicon-chevron-down"),$(".mostraComentarios span").addClass("glyphicon glyphicon-chevron-up")):$(".mostraComentarios span").hasClass("glyphicon glyphicon-chevron-up")&&($(".mostraComentarios span").removeClass("glyphicon glyphicon-chevron-up"),$(".mostraComentarios span").addClass("glyphicon glyphicon-chevron-down"))});$(".info_c-studio").click(function(){$(".layerInfo_c-studio").fadeToggle()})},exit:function(){}},{breakpoint:"tablet",enter:function(){$(".commentsBtn").click(function(){$("html, body").animate({scrollTop:$("#scroll").offset().top-($("header").height()+60)},"slow")});setTimeout(function(){Galleria.loadTheme("/Scripts/themes/classic/galleria.classic.min.js");Galleria.configure({height:.76,imageCrop:!1,responsive:!0,suggestedUrl:"#",extend:function(){Galleria.get(0).$("info-link").click()}})},500);$("#partilhaSticky").length>0&&$(window).scroll(function(){partilhasNoticia();partilhas_FixarBotom()});$(".info_c-studio").click(function(){$(".layerInfo_c-studio").fadeToggle()})},exit:function(){}},{breakpoint:"pc",enter:function(){$(".commentsBtn").click(function(){$("html, body").animate({scrollTop:$("#scroll").offset().top-($("header").height()+60)},"slow")});setTimeout(function(){Galleria.loadTheme("/Scripts/themes/classic/galleria.classic.min.js");Galleria.configure({debug:!1,wait:!0,height:.76,imageCrop:!1,responsive:!0,suggestedUrl:"#",extend:function(){Galleria.get(0).$("info-link").click()}})},500);$("#partilhaSticky").length>0&&$(window).scroll(function(){partilhasNoticia();partilhas_FixarBotom()});$(".info_c-studio").hover(function(){$(".layerInfo_c-studio").fadeToggle()});$(function(){$(".pub_stick").length>0&&$(".pub_stick").each(function(){if($(this).parents(".col_styck").length>0)$("#pub_stick").stick_in_parent({parent:".col_styck",spacer:!1,offset_top:140,recalc_every:!0,bottoming:!0}).on("sticky_kit:bottom",function(){}).on("sticky_kit:unbottom",function(){})})})},exit:function(){}}]);$(".blocoGaleria")!=null&&$(".blocoGaleria").length>0&&Galleria.run(".blocoGaleria");$(function(){function n(){var n=$(this),t=n.find(".modal-dialog");n.css("display","block");t.css("margin-top",Math.max(0,($(window).height()-t.height())/2))}$(".modal").on("show.bs.modal",n);$(window).on("resize",function(){$(".modal:visible").each(n)})});$(document).on("click",".btnLerMais",function(){$(".lerMais").hide();$(".destaqueRelacionadas").length>0&&$(".destaqueRelacionadas").show();var n=$(".mrecContainerNoDisplay");n!=null&&n.length>0&&n.each(function(){COF.pubApi.renderInContent($(this))});$(".showLerMais").fadeIn(300)});setTimeout(function(){$("#partilhaSticky").removeClass("utilitiesFixedBottom");$("#partilhaSticky").removeClass("utilitiesFixed")},150);$(function(){var t=$(".noticiaRelacionada img[data-contentid]"),n;t!=null&&typeof t!="undefined"&&t.length>0&&(n=[],$.each(t,function(t,i){n.push(i.getAttribute("data-contentid"))}),n!=null&&n.length>0&&$.ajax({type:"GET",url:"/Detalhe/GetRelatedContentImageUrl",data:{contentIds:n},traditional:!0,success:function(n){$.each(n,function(n,t){$(".noticiaRelacionada img[data-contentid='"+n+"']").attr("src",t)})}}))})